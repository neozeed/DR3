ident		OSF

include		"HP700/config.common"
include		"HP700/config.devices"
include		"HP700/config.debug"
include		"HP700/config.gdb"
include		"config.net"

config		"mach_kernel.DEBUG+net"

timezone	5 dst
maxusers	16

makeoptions	CC_OPT_LEVEL="-g"

#
# Options:
# The XK_TEST option includes test protocols to be activated
# by the kernel debugger
#
options		XK_TEST

#
# Options:
# The XK_DEBUG adds tracing capabilities and other debugging hooks.
#
options		XK_DEBUG
